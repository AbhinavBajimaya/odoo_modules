	<template id="snippet_options_inherited" inherit_id="website.snippet_options">
		
		<!-- Headers -->
		<xpath expr="//we-select[@data-variable='header-template']" position="inside">
            <we-button title="Vouge Header 1"
                       data-name="vouge_header_one_opt"
                       data-customize-website-views="theme_nextbiz.theme_nextbiz_header_new1"
                       data-customize-website-variable="'vouge-header-1'"
                       data-img="/theme_nextbiz/static/description/header1.png"/>
            <we-button title="Vouge Header 2"
                       data-name="vouge_header_two_opt"
                       data-customize-website-views="theme_nextbiz.theme_nextbiz_header_new2"
                       data-customize-website-variable="'vouge-header-2'"
                       data-img="/theme_nextbiz/static/description/header2.png"/>
            <we-button title="Vouge Header 3"
                       data-name="vouge_header_three_opt"
                       data-customize-website-views="theme_nextbiz.theme_nextbiz_header_new3,website.no_autohide_menu"
                       data-customize-website-variable="'sidebar'"
                       data-img="/theme_nextbiz/static/description/header3.png"/>
            <we-button title="Vouge Header 8"
                       data-name="vouge_header_eight_opt"
