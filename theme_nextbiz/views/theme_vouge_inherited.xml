                       data-customize-website-variable="'vouge-header-10'"
                       data-img="/theme_nextbiz/static/description/header6.png"/>
        </xpath>
		<xpath expr="//div[@data-js='HeaderNavbar']//we-select" position="replace">
                <we-select string="Alignment"
                    data-name="header_alignment_opt"
                    data-variable="hamburger-position"
                    data-reload="/">
                <we-button data-customize-website-views="" data-customize-website-variable="'left'">Left</we-button>
                <we-button data-customize-website-views="website.template_header_default_align_center, website.template_header_hamburger_align_center, website.template_header_slogan_align_center, theme_nextbiz.template_header_one_align_center,theme_nextbiz.template_header_two_align_center,theme_nextbiz.template_header_three_align_center,theme_nextbiz.template_header_four_align_center,theme_nextbiz.template_header_five_align_center,theme_nextbiz.template_header_six_align_center" data-customize-website-variable="'center'">Center</we-button>
