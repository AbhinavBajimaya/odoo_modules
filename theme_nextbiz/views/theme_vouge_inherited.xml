            </we-select>
        </xpath>

    <xpath expr="//we-select[@data-variable='header-template']" position="after">
      <we-input string="⌙ Width"
                  data-dependencies="vouge_header_three_opt"
                  data-customize-website-variable="null"
                  data-variable="sidebar-width"
                  data-unit="px"
                  data-save-unit="rem"/>
    </xpath>
	<xpath expr="//div[@data-js='topMenuColor']" position="inside">
